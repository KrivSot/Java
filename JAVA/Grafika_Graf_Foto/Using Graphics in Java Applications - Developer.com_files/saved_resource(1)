/* 284 */             $('.login-content').css("width",leadformData['width'] - 20);
/* 285 */             $('#loginLightBox').css("width",leadformData['width'] - 20);
/* 286 */         }
/* 287 */         welcomeUser();
/* 288 */ 	}
