/* 230 */ function welcomeUser(){
/* 231 */ 	if($.cookie('fn')!=null && $.cookie('ln')!=null){
/* 232 */ 			var firstName = decodeURIComponent($.cookie('fn'));
/* 233 */ 			var lastName =  decodeURIComponent($.cookie('ln'));
/* 234 */ 			firstName = firstName.replace("+"," ");
/* 235 */ 			lastName = lastName.replace("+"," ");
/* 236 */ 			if ($('#custom-developer-login-register').css('display') == "block"){
/* 237 */ 				$('#custom-developer-logout').show();
/* 238 */ 				$('#custom-developer-login-register').hide();
/* 239 */ 				$("<span class='greeting' style='color: #797979; font-family: Arial, sans-serif; font-size: 13px;'>Hi, <a href='/accountManagement?formType=userProfile' alt='User Profile' title='User Profile'>"+firstName+' '+lastName+"</a>&nbsp;&nbsp;</span>").insertBefore("#custom-developer-logout .logout-link");
/* 240 */ 			}
/* 241 */ 		}else{
/* 242 */ 				$('#custom-developer-logout').hide();
/* 243 */ 				$('#custom-developer-login-register').show();
/* 244 */ 		}	
/* 245 */ }
/* 246 */ 
/* 247 */ var userId = '';
/* 248 */ 	if($.cookie('USERID_COOKIE')!=null){
/* 249 */ 		userId =  decodeURIComponent($.cookie('USERID_COOKIE'));
/* 250 */ 	}

/* common.js */

/* 251 */ 	else if($.cookie('STAGE_USERID_COOKIE')!=null && userId==''){
/* 252 */ 		userId =  decodeURIComponent($.cookie('STAGE_USERID_COOKIE'));		
/* 253 */ 	}
/* 254 */ 	else if($.cookie('TEST_USERID_COOKIE')!=null && userId==''){
/* 255 */ 		userId =  decodeURIComponent($.cookie('TEST_USERID_COOKIE'));		
/* 256 */ 	}
/* 257 */ 	else if($.cookie('DEV_USERID_COOKIE')!=null && userId==''){
/* 258 */ 		userId =  decodeURIComponent($.cookie('DEV_USERID_COOKIE'));		
/* 259 */ 	}
/* 260 */ 	else
/* 261 */ 		userId = 'undefined';
/* 262 */ 	
/* 263 */ 	if(userId!='undefined'){
/* 264 */ 		var meta = document.createElement('meta');
/* 265 */ 		meta.name = 'DCS.dcsaut';		
/* 266 */ 		meta.content = userId;
/* 267 */ 		document.getElementsByTagName('head')[0].appendChild(meta);	
/* 268 */ 	}
