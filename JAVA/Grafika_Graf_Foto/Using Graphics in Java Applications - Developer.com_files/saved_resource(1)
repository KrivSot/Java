/* tabs.js */

/* 1  */ var tabs = new Array();
/* 2  */ 
/* 3  */ oldOnLoad = window.onload;
/* 4  */ if (typeof oldOnLoad == "function") {
/* 5  */ 	window.onload = function() {
/* 6  */ 		oldOnLoad();
/* 7  */ 		initTabs();
/* 8  */ 	};
/* 9  */ } else {
/* 10 */ 	window.onload = function() {
/* 11 */ 		initTabs();
/* 12 */ 	};
/* 13 */ }
/* 14 */ 
/* 15 */ var initTabs = function() {
/* 16 */ 	all = document.getElementsByTagName("DIV");
/* 17 */ 	var j = 0;
/* 18 */ 	for(var i=0;i<all.length;i++) {
/* 19 */ 		if (hasClass(all[i],"tabs")) {
/* 20 */ 			
/* 21 */ 			var menu = searchChilds(all[i],"tabsMenu");
/* 22 */ 			var contents = searchChilds(all[i],"tabsContent");
/* 23 */ 			
/* 24 */ 			if (menu.length > 0 && contents.length > 0) {
/* 25 */ 				tabs[j] = {};
/* 26 */ 				tabs[j].e = all[i];
/* 27 */ 				tabs[j].menu = menu;
/* 28 */ 				tabs[j].contents = menu;
/* 29 */ 				tabs[j].current = undefined;
/* 30 */ 				j++;
/* 31 */ 			}
/* 32 */ 		}
/* 33 */ 	}
/* 34 */ 	
/* 35 */ 	for(var i=0;i<tabs.length;i++) {
/* 36 */ 		
/* 37 */ 		
/* 38 */ 		var t = new function() {
/* 39 */ 			var tab = tabs[i];
/* 40 */ 			for(var j=0;j<tab.menu.length;j++) {
/* 41 */ 				var menus = tab.menu[j].getElementsByTagName('A');
/* 42 */ 				for(var m=0;m<menus.length;m++) {
/* 43 */ 					if (hasClass(menus[m],"selected")) {
/* 44 */ 						showTab(tab,menus[m]);
/* 45 */ 					}
/* 46 */ 					menus[m].onclick = function() {
/* 47 */ 						hideTab(tab);
/* 48 */ 						showTab(tab,this);
/* 49 */ 						return false;
/* 50 */ 					}

/* tabs.js */

/* 51 */ 				}
/* 52 */ 			}
/* 53 */ 		}
/* 54 */ 		
/* 55 */ 		
/* 56 */ 	}
/* 57 */ }
/* 58 */ 
/* 59 */ var showTab = function(tab,obj) {
/* 60 */ 	var h = obj.href;
/* 61 */ 	var a = h.split("#",2);
/* 62 */ 	if (a.length > 1) {
/* 63 */ 		addClass(obj,"selected");
/* 64 */ 		document.getElementById(a[1]).style.display="block";
/* 65 */ 	}
/* 66 */ 	tab.current = obj;
/* 67 */ 	
/* 68 */ }
/* 69 */ 
/* 70 */ var hideTab = function(tab) {
/* 71 */ 	var obj = tab.current;
/* 72 */ 	var h = obj.href;
/* 73 */ 	var a = h.split("#",2);
/* 74 */ 	if (a.length > 1) {
/* 75 */ 		removeClass(obj,"selected");
/* 76 */ 		removeClass(document.getElementById(a[1]),"selected");
/* 77 */ 		document.getElementById(a[1]).style.display="none";
/* 78 */ 	}
/* 79 */ 	
/* 80 */ }
