/* article_menu.js */

/* 1  */ var initArticleMenu = function(element) {
/* 2  */ 	if (element == undefined) return false;
/* 3  */ 
/* 4  */ 	var m = new function() {
/* 5  */ 		var self = this;
/* 6  */ 		var bOver = false;
/* 7  */ 		var aTrigger = undefined;
/* 8  */ 		var subMenu = undefined;
/* 9  */ 		var ele = element;
/* 10 */ 		var hideTimeout = 0;
/* 11 */ 
/* 12 */ 		var onMouseOver = function() {
/* 13 */ 			bOver = true;
/* 14 */ 			cancelHide();
/* 15 */ 			doShow();
/* 16 */ 		};
/* 17 */ 
/* 18 */ 		var onMouseOut = function() {
/* 19 */ 			bOver = false;
/* 20 */ 			initHide();
/* 21 */ 		};
/* 22 */ 
/* 23 */ 		var onSubMenuMouseOver = function() {
/* 24 */ 			bOver = true;
/* 25 */ 			cancelHide();
/* 26 */ 			doShow();
/* 27 */ 		};
/* 28 */ 
/* 29 */ 		var onSubMenuMouseOut = function() {
/* 30 */ 			bOver = false;
/* 31 */ 			initHide();
/* 32 */ 		};
/* 33 */ 
/* 34 */ 		var doShow = function() {
/* 35 */ 			ele.style.backgroundPosition = "-128px -5px";
/* 36 */ 			if (subMenu) subMenu.style.display = "block";
/* 37 */ 		};
/* 38 */ 
/* 39 */ 		var doHide = function() {
/* 40 */ 			cancelHide();
/* 41 */ 			if (!bOver) {
/* 42 */ 				ele.style.backgroundPosition = "0px -1000px";
/* 43 */ 				if (subMenu) subMenu.style.display = "none";
/* 44 */ 			}
/* 45 */ 		};
/* 46 */ 		var initHide = function() {
/* 47 */ 			cancelHide();
/* 48 */ 			hideTimeout = window.setTimeout(doHide,300);
/* 49 */ 		};
/* 50 */ 		var cancelHide = function() {

/* article_menu.js */

/* 51 */ 			if (hideTimeout > 0) window.clearTimeout(hideTimeout);
/* 52 */ 			hideTimeout = 0;
/* 53 */ 		};
/* 54 */ 
/* 55 */ 		aTrigger = searchChildsByTagName(ele, "A", 0, 1);
/* 56 */ 		if (aTrigger.length > 0) {
/* 57 */ 			aTrigger = aTrigger[0];
/* 58 */ 
/* 59 */ 			aTrigger.onmouseover = onMouseOver;
/* 60 */ 			aTrigger.onmouseout = onMouseOut;
/* 61 */ 		}
/* 62 */ 
/* 63 */ 		subMenu = searchChilds(ele,"shareSubMenu");
/* 64 */ 		if (subMenu.length > 0) {
/* 65 */ 			subMenu = subMenu[0];
/* 66 */ 			subMenu.onmouseover = onSubMenuMouseOver;
/* 67 */ 			subMenu.onmouseout = onSubMenuMouseOut;
/* 68 */ 		} else {
/* 69 */ 			subMenu = undefined;
/* 70 */ 		}
/* 71 */ 	}
/* 72 */ 
/* 73 */ 	return m;
/* 74 */ }

