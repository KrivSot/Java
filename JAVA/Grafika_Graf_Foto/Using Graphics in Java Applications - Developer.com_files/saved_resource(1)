/* 316 */ function registrationPopup() {
/* 317 */     if($.cookie('USERID_COOKIE')!=null && $.cookie('newuserregistration')=='y') {
/* 318 */         $.cookie("newuserregistration", 'n', { path: '/',domain:'.developer.com'});
/* 319 */         $("#newuserregistrationpopup").lightbox_me({
/* 320 */             centered: true,
/* 321 */             preventScroll: true,
/* 322 */             overlayCSS: {background: '#595959','opacity': 0.8},
/* 323 */             onClose: function   () {
/* 324 */                 $.cookie("newuserregistration", 'n', { path: '/',domain:'.developer.com'});
/* 325 */             }
/* 326 */         });
/* 327 */         setTimeout(function(){
/* 328 */             $("#newuserregistrationpopup").trigger("close");
/* 329 */             window.location.href = '/';
/* 330 */         }, 30000);
/* 331 */     }
/* 332 */ }
