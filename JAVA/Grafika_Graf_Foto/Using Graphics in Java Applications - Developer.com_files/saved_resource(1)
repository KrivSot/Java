/* 85 */ 			{ regex: new RegExp(this.getKeywords(functions), 'gm'),		css: 'functions bold' },
/* 86 */ 			{ regex: new RegExp(this.getKeywords(keywords), 'gm'),		css: 'keyword bold' }
