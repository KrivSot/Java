/* 183 */ /*!
/* 184 *| * jQuery Cookie Plugin
/* 185 *| */
/* 186 */ 	(function($) {
/* 187 */ 		$.cookie = function(key, value, options) {
/* 188 */ 
/* 189 */ 			// key and at least value given, set cookie...
/* 190 */ 			if (arguments.length > 1 && (!/Object/.test(Object.prototype.toString.call(value)) || value === null || value === undefined)) {
/* 191 */ 				options = $.extend({}, options);
/* 192 */ 
/* 193 */ 				if (value === null || value === undefined) {
/* 194 */ 					options.expires = -1;
/* 195 */ 				}
/* 196 */ 
/* 197 */ 				if (typeof options.expires === 'number') {
/* 198 */ 					var days = options.expires, t = options.expires = new Date();
/* 199 */ 					t.setDate(t.getDate() + days);
/* 200 */ 				}

/* common.js */

/* 201 */ 
/* 202 */ 				value = String(value);
/* 203 */ 
/* 204 */ 				return (document.cookie = [
/* 205 */ 					encodeURIComponent(key), '=', options.raw ? value : encodeURIComponent(value),
/* 206 */ 					options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
/* 207 */ 					options.path ? '; path=' + options.path : '',
/* 208 */ 					options.domain ? '; domain=' + options.domain : '',
/* 209 */ 					options.secure ? '; secure' : ''
/* 210 */ 				].join(''));
/* 211 */ 			}
/* 212 */ 
/* 213 */ 			// key and possibly options given, get cookie...
/* 214 */ 			options = value || {};
/* 215 */ 			var decode = options.raw ? function(s) { return s; } : decodeURIComponent;
/* 216 */ 
/* 217 */ 			var pairs = document.cookie.split('; ');
/* 218 */ 			for (var i = 0, pair; pair = pairs[i] && pairs[i].split('='); i++) {
/* 219 */ 				if (decode(pair[0]) === key) return decode(pair[1] || ''); // IE saves cookies with empty string as "c; ", e.g. without "=" as opposed to EOMB, thus pair[1] may be undefined
/* 220 */ 			}
/* 221 */ 			return null;
/* 222 */ 		};
/* 223 */ 	})(jQuery);
