
/* common.js */

/* 1   */ var getBrowser = function() {
/* 2   */ 	var userAgent = navigator.userAgent.toLowerCase();
/* 3   */ 	if (/webkit/.test( userAgent )) {
/* 4   */ 		return "safari";
/* 5   */ 	}
/* 6   */ 	if (/opera/.test( userAgent )) {
/* 7   */ 		return "opera";
/* 8   */ 	}
/* 9   */ 	if (/msie/.test( userAgent ) && !/opera/.test( userAgent )) {
/* 10  */ 		return "msie";
/* 11  */ 	}
/* 12  */ 	if (/mozilla/.test( userAgent ) && !/(compatible|webkit)/.test( userAgent )) {
/* 13  */ 		return "mozilla";
/* 14  */ 	}
/* 15  */ };
/* 16  */ 
/* 17  */ var getBrowserVersion = function() {
/* 18  */ 	var userAgent = navigator.userAgent.toLowerCase();
/* 19  */ 	return (userAgent.match( /.+(?:rv|it|ra|ie)[\/: ]([\d.]+)/ ) || [])[1];
/* 20  */ };
/* 21  */ 
/* 22  */ var hasClass = function(element, className) {
/* 23  */ 	if ((className == "") || (className == null)) return false;
/* 24  */ 	if ((element == null) || (element == "")) return false;
/* 25  */ 	var array = (element.className || element).toString().split(/\s+/);
/* 26  */ 	
/* 27  */ 	var j=-1;
/* 28  */ 	for (var i=0;i<array.length;i++) {
/* 29  */ 		if (array[i] === className) {
/* 30  */ 			j=i;
/* 31  */ 		}
/* 32  */ 	}
/* 33  */ 	return (j > -1);
/* 34  */ }
/* 35  */ 
/* 36  */ var removeClass = function(element, className) {
/* 37  */         if ((className == "") || (className == null)) return false;
/* 38  */         if ((element == null) || (element == "")) return false;
/* 39  */ 	var arr = (element.className || element).toString().split(/\s+/);
/* 40  */ 	
/* 41  */ 	var newArr = new Array();
/* 42  */ 	var j = 0;
/* 43  */ 	for (var i=0;i<arr.length;i++) {
/* 44  */ 		if (arr[i] != className) {
/* 45  */ 			newArr[j] = arr[i];
/* 46  */ 			j++;
/* 47  */ 		}
/* 48  */ 	}
/* 49  */ 	element.className = newArr.join(" ");
/* 50  */ }

/* common.js */

/* 51  */ 
/* 52  */ var addClass = function(element, className) {
/* 53  */         if ((className == "") || (className == null)) return false;
/* 54  */         if ((element == null) || (element == "")) return false;
/* 55  */ 	if (!hasClass(element,className)) {
/* 56  */ 		element.className += (element.className ? " " : "") + className;
/* 57  */ 	}
/* 58  */ }
/* 59  */ 
/* 60  */ var searchChilds = function(element, childClassName, level, maxLevel) {
/* 61  */ 	maxLevel = maxLevel || 0;
/* 62  */ 	level = level || 0;
/* 63  */ 	
/* 64  */ 	var childs = new Array();
/* 65  */ 	for(var i=0;i<element.childNodes.length;i++) {
/* 66  */ 		if (hasClass(element.childNodes[i],childClassName)) {
/* 67  */ 			childs.push(element.childNodes[i]);
/* 68  */ 		}
/* 69  */ 		if (element.childNodes[i].childNodes.length > 0 && (maxLevel == 0 || level < maxLevel)) {
/* 70  */ 			childs = childs.concat(searchChilds(element.childNodes[i],childClassName,level+1,maxLevel));
/* 71  */ 		}
/* 72  */ 	}
/* 73  */ 	return childs;
/* 74  */ }
/* 75  */ 
/* 76  */ var searchChildsByTagName = function(element, childTagName, level, maxLevel) {
/* 77  */ 	maxLevel = maxLevel || 0;
/* 78  */ 	level = level || 0;
/* 79  */ 	
/* 80  */ 	var childs = new Array();
/* 81  */ 	for(var i=0;i<element.childNodes.length;i++) {
/* 82  */ 		if (element.childNodes[i].tagName == childTagName) {
/* 83  */ 			childs.push(element.childNodes[i]);
/* 84  */ 		}
/* 85  */ 		if (element.childNodes[i].childNodes.length > 0 && (maxLevel == 0 || level < maxLevel)) {
/* 86  */ 			childs = childs.concat(searchChildsByTagName(element.childNodes[i],childTagName,level+1,maxLevel));
/* 87  */ 		}
/* 88  */ 	}
/* 89  */ 	return childs;
/* 90  */ }
/* 91  */ 
/* 92  */ function getStyleValue(elem, prop) {
/* 93  */ 	var ret, style = elem.style;
/* 94  */ 	
/* 95  */ 	if (prop == "opacity" && getBrowser() == "msie") {
/* 96  */ 		return style.filter && style.filter.indexOf("opacity=") >= 0 ?
/* 97  */ 			(parseFloat( style.filter.match(/opacity=([^)]*)/)[1] ) / 100) + '':
/* 98  */ 			"";
/* 99  */ 	} else {
/* 100 */ 		return parseInt(style[prop],10) || 0;

/* common.js */

/* 101 */ 	}
/* 102 */ }
/* 103 */ 
/* 104 */ function setStyleValue(elem, prop, value) {	
/* 105 */ 	if (prop == "opacity" && getBrowser() == "msie") {
/* 106 */ 		elem.style.zoom=1;
/* 107 */ 		elem.style.filter = (elem.style.filter || "").replace( /alpha\([^)]*\)/, "" ) +
/* 108 */ 			(parseInt( value ) + '' == "NaN" ? "" : "alpha(opacity=" + value * 100 + ")");
/* 109 */ 		
/* 110 */ 	} else {
/* 111 */ 		elem.style[prop]=value;
/* 112 */ 	}
/* 113 */ }
/* 114 */ 
/* 115 */ function getWidth(elem, p, b, m) {
/* 116 */ 	w = elem.offsetWidth;
/* 117 */ 	
/* 118 */ 	p = p || true;
/* 119 */ 	b = b || true;
/* 120 */ 	m = m || false;
/* 121 */ 	
/* 122 */ 	padding = 0;
/* 123 */ 	border = 0;
/* 124 */ 	margin = 0;
/* 125 */ 	
/* 126 */ 	if (p)
/* 127 */ 		padding = getStyleValue(elem, "paddingLeft") + getStyleValue(elem, "paddingRight");
/* 128 */ 		
/* 129 */ 	if (b)
/* 130 */ 		border = getStyleValue(elem, "borderLeftWidth") + getStyleValue(elem, "borderRightWidth");
/* 131 */ 		
/* 132 */ 	if (m) 
/* 133 */ 		margin = getStyleValue(elem, "marginLeft") + getStyleValue(elem, "marginRight");
/* 134 */ 		
/* 135 */ 	return w + padding + border + margin;
/* 136 */ }
/* 137 */ 
/* 138 */ function now() {
/* 139 */ 	return +new Date;
/* 140 */ }
/* 141 */ 
/* 142 */ var Animation = function(duration,step,complete,cancel) {
/* 143 */ 	var startTime = now();
/* 144 */ 	var dur = duration;
/* 145 */ 	var self=this;
/* 146 */ 	
/* 147 */ 	var timerId = window.setInterval(function() {
/* 148 */ 		var n  = now() - startTime;
/* 149 */ 		var state = n / dur;
/* 150 */ 		if (state > 1) {

/* common.js */

/* 151 */ 			state = 1;
/* 152 */ 		}
/* 153 */ 		if (step != undefined && typeof(step) == "function") {
/* 154 */ 			
/* 155 */ 			var r = step.call(self,state,n,dur) || true;
/* 156 */ 			if (!r || state >= 1) {
/* 157 */ 				window.clearInterval(timerId);
/* 158 */ 				if (!r && cancel != undefined && typeof(cancel) == "function") {
/* 159 */ 					cancel.call(state);
/* 160 */ 				}
/* 161 */ 				if (r && complete != undefined && typeof(complete) == "function") {
/* 162 */ 					complete.call(state);
/* 163 */ 				}
/* 164 */ 			}
/* 165 */ 		}
/* 166 */ 	},13);
/* 167 */ 	
/* 168 */ }
/* 169 */ 
/* 170 */ function setVisibility(id, visibility) {
/* 171 */     if (typeof(document.getElementById(id+'Dropdown')) != 'undefined' && document.getElementById(id+'Dropdown') != null)
/* 172 */     {
/* 173 */         document.getElementById(id+'Dropdown').style.display = visibility;
/* 174 */         if(visibility == 'inline') {
/* 175 */             $('#'+id+'Head').addClass("hilightedHead");
/* 176 */         } else {
/* 177 */             $('#'+id+'Head').removeClass("hilightedHead");
/* 178 */         }
/* 179 */     }
/* 180 */ }
